{
  "name": "blx32/laravel-paghiper",
  "description": "Biblioteca de integração Laravel do PagHiper",
  "keywords": [
    "paghiper",
    "laravel"
  ],
  "require": {
    "php": "^7.2|^8.0",
    "ext-openssl": "*",
    "guzzlehttp/guzzle": "^6.5|^7.0",
    "webmasterdro/paghiper-php-sdk": "^3.0",
    "laravel/cashier": "^12.5",
    "illuminate/database": "^7.9|^8.0",
    "illuminate/routing": "^7.9|^8.0"
  },
  "autoload": {
    "psr-4": {
      "Blx32\\LaravelPagHiper\\": "src/"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Gabriel Moura",
      "email": "develop@srmoura.com.br"
    }
  ],
  "extra": {
    "component": "package",
    "frameworks": ["Laravel"],
    "laravel": {
      "providers": [
        "Blx32\\LaravelPagHiper\\PagHiperServiceProvider"
      ],
      "aliases": {
        "PagHiper": "Blx32\\LaravelPagHiper\\PagHiperFacade"
      }
    }
  }
}
